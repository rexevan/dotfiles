! -- Colors -------------------------------------------------------- {{{

! Base
*background: #212121
*foreground: #d9d9d9
*cursorColor: #20bbfc
! Black
*color0: #212121
*color8: #424242
! Red
*color1: #c30771
*color9: #fb007a
! Green
*color2: #10a778
*color10: #5fd7af
! Yellow
*color3: #a89c14
*color11: #f3e430
! Blue
*color4: #008ec4
*color12: #20bbfc
! Magenta
*color5: #523c79
*color13: #6855de
! Cyan
*color6: #20a5ba
*color14: #4fb8cc
! White
*color7: #d9d9d9
*color15: #f1f1f1

! }}}

! vim:foldmethod=marker:filetype=xdefaults


URxvt*fading:  30
URxvt*scrollBar:  false
URxvt*colorUL:  yellow
URxvt*font: xft:Ubuntu Mono:size=12
URxvt*boldFont: xft:Ubuntu Mono:bold:autohint=true:size=12
URxvt*italicFont: xft:Ubuntu Mono:italic:autohint=true:size=12
URxvt*boldItalicFont: xft:Ubuntu Mono:bold:italic:autohint=true:size=12

URxvt*perl-ext:
URxvt*perl-ext-common:
URxvt*.termName: screen-256color

! FAKE transparency
urxvt*depth: 32
!urxvt*background: rgba:0000/0000/0200/c800
